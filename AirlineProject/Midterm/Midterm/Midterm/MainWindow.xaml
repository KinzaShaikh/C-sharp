<Window x:Class="Midterm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Midterm"
        mc:Ignorable="d"
        Title="mainWindow" Height="275" Width="360" WindowStartupLocation="CenterScreen" Background="#FF130404" HorizontalAlignment="Center" VerticalAlignment="Center" ResizeMode="NoResize" RenderTransformOrigin="0.5,0.5">
    <Window.RenderTransform>
        <TransformGroup>
            <ScaleTransform/>
            <SkewTransform/>
            <RotateTransform/>
            <TranslateTransform X="0" Y="0"/>
        </TransformGroup>
    </Window.RenderTransform>
    <Grid Height="434" VerticalAlignment="Bottom">
        <TextBlock Text="TextBlock" TextWrapping="Wrap" RenderTransformOrigin="0.498,0.617"/>

    </Grid>
</Window>
